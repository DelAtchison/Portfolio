#gallery[data-page="default"] .holdercontent:hover + img, #gallery[data-page="default"] .thumbnail:hover {
  -webkit-transform: scale(1.1, 1.1);
  -moz-transform: scale(1.1, 1.1);
  -ms-transform: scale(1.1, 1.1);
  -o-transform: scale(1.1, 1.1);
  transition: all 0.2s ease-in-out;
  -o-transition: all 0.2s ease-in-out;
  -ms-transition: all 0.2s ease-in-out;
  -moz-transition: all 0.2s ease-in-out;
  -webkit-transition: all 0.2s ease-in-out;
  -o-transition: all 0.2s ease-in-out;
  filter: blur(3px) hue-rotate(325deg) sepia(1);
  -o-filter: sepia(1) hue-rotate(325deg) blur(3px);
  -ms-filter: sepia(1) hue-rotate(325deg) blur(3px);
  -moz-filter: sepia(1) hue-rotate(325deg) blur(3px);
